  "version": "1.0.0",
  "description": "react-webpack demo",
  "main": "app.js",
  "scripts": {
    "test": "jest --coverage",
    "start": "webpack-dev-server",
    "build": "webpack",
    "production": "webpack --config ./webpack.production.config.js --progress",
    "dev": "NODE_ENV=development webpack-dev-server  --inline --progress --colors --port 3003 --host 0.0.0.0 "
